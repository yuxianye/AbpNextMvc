@page
@inherits Solution.Web.Pages.SolutionPage
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model Solution.Web.Pages.Equipments.EquipmentMaintenance.EditModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="EquipmentMaintenance" data-ajaxForm="true" asp-page="EditModal">
    <abp-modal>
        <abp-modal-header title="@L["EditEquipmentMaintenance"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-input asp-for="Id" />
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>